<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/content/ui/global/bootstrap/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/content/ui/global/font-awesome/css/font-awesome.css")" />
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/content/adminlte/dist/css/AdminLTE.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/content/adminlte/dist/css/skins/_all-skins.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/content/min/css/supershopui.common.min.css")" />
    <script src="@Url.Content("~/Content/plugins/datepicker/WdatePicker.js")"></script>
    <style type="text/css">
        .my-container {
            float: left;
            display: inline-block;
            margin-right: 30px;
        }

        .myLabel-content, .myText-content, .myBtn-content {
            float: left;
            display: inline-block;
            margin-left: 10px;
        }

            .myLabel-content, .myText-content input[type='text'], .myBtn-content .btn {
                height: 30px;
                font-size: 12px;
            }

            .myBtn-content .btn {
                margin-bottom: 10px;
            }
    </style>
    @RenderSection("Styles", false)
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    @RenderBody()
    <!-- ./wrapper -->
    <!-- REQUIRED JS SCRIPTS -->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content("~/content/ui/global/jQuery/jquery-2.2.3.min.js")"></script>
    <script src="@Url.Content("~/content/ui/global/bootstrap/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/content/min/js/supershopui.common.js")"></script>
    <script src="@Url.Content("/Content/plugins/jqgrid/grid.locale-cn.js")"></script>
    <script src="@Url.Content("/Content/plugins/jqgrid/jqgrid.js")"></script>
    <script src="@Url.Content("~/content/supermgr/Base/js/common.js")"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content("~/content/plugins/validator/validator.js")"></script>
    <script src="@Url.Content("~/content/plugins/layout/jquery.layout.js")"></script>
    <script src="@Url.Content("~/Content/plugins/wdtree/tree.js")"></script>
    <!--Script references. -->
    <!--The jQuery library is required and is referenced by default in _Layout.cshtml. -->
    <script type="text/javascript">
        function lockFormatter(value) {
            if (value == 1 || value == true) return '<span class=\"label label-success\">是</span>';
            else return '<span class=\"label label-default\">否</span>';
        }
        function dataFormatter(value) {
            if (value == 1) return '<span class=\"label-success\">正常</span>';
            else return '<span class=\"label-danger\">失败</span>';
        }
        function urlFormatter(value, row, index) {
            return [
                '<a href="' + value + '" target="_blank">' + value + '</a>'
            ].join('');
        }
    </script>
    @RenderSection("Scripts", false)
</body>
</html>
